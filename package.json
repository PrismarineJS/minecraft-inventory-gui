  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PrismarineJS/minecraft-inventory-gui/issues"
  },
  "homepage": "https://github.com/PrismarineJS/minecraft-inventory-gui#readme",
  "devDependencies": {
